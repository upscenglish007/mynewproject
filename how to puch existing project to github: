$ mkdir localrepository

VERSION IT@DESKTOP-19E754H MINGW64 /e
$ touch file1

VERSION IT@DESKTOP-19E754H MINGW64 /e
$ touch file2 file3 file4

VERSION IT@DESKTOP-19E754H MINGW64 /e
$ vi file1

VERSION IT@DESKTOP-19E754H MINGW64 /e
$ cat file1
this is new file akhilesh



VERSION IT@DESKTOP-19E754H MINGW64 /e
$ cd localrepository/

VERSION IT@DESKTOP-19E754H MINGW64 /e/localrepository
$ touch file1 file2 file3 file4

VERSION IT@DESKTOP-19E754H MINGW64 /e/localrepository
$ vi file1

VERSION IT@DESKTOP-19E754H MINGW64 /e/localrepository
$ cat file1
this is first file


